    "/api/v1/agents/create": {
      "post": {
        "operationId": "create-personalized-agent",
        "x-zuplo-route": {
          "corsPolicy": "none",
          "handler": {
            "export": "urlRewriteHandler",
            "module": "$import(@zuplo/runtime)",
            "options": {
              "rewritePattern": "${env.AGENT_MANAGEMENT_URL}/api/agents/create",
              "forwardSearch": false
            }
          },
          "policies": {
            "inbound": ["user-jwt-auth", "add-auth-headers"]
          }
        },
        "summary": "Create personalized agent",
        "description": "Create a personalized Letta agent via Agent Management Service",
        "tags": ["Agent Management"],
        "security": [{"UserJWT": []}],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "personalInfo": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string",
                        "description": "User's name",
                        "example": "Иван Петров"
                      },
                      "interests": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        },
                        "description": "User's interests",
                        "example": ["программирование", "искусственный интеллект"]
                      },
                      "communicationStyle": {
                        "type": "string",
                        "description": "Preferred communication style",
                        "example": "дружелюбный и профессиональный"
                      },
                      "background": {
                        "type": "string",
                        "description": "User's background",
                        "example": "Software Engineer с 5-летним опытом"
                      },
                      "goals": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        },
                        "description": "User's goals",
                        "example": ["изучить новые технологии", "повысить продуктивность"]
                      },
                      "projects": {
                        "type": "array",
                        "items": {
                          "type": "object",
